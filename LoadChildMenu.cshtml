@model IEnumerable<RestaurantFood.Models.MENU>
@if ((int)ViewBag.Count > 0)
{
    <ul class="header__menu__dropdown">
        @foreach (var item in Model)
        {
            <li>
                <a href="@item.MenuLink">@item.MenuName</a>
                @Html.Action("LoadChildMenu1", "Home", new { parentId = item.Id })
            </li>
        }
    </ul>
}



